# keystore.properties.example
# Bu dosyayı keystore.properties olarak kopyalayın ve değerleri doldurun

# Google Maps API Key - Google Cloud Console'dan alın
MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Keystore bilgileri (release build için)
KEYSTORE_FILE=keystore.jks
KEYSTORE_PASSWORD=your_keystore_password_here
KEY_ALIAS=key0
KEY_PASSWORD=your_key_password_here

# Diğer API anahtarları
MAPBOX_API_KEY=YOUR_MAPBOX_API_KEY_HERE
OPENWEATHERMAP_API_KEY=YOUR_OPENWEATHER_API_KEY_HERE
APPCENTER_ANDROID_SECRET=YOUR_APPCENTER_ANDROID_SECRET
PAYPAL_BUTTON_ID=YOUR_PAYPAL_BUTTON_ID
STRIPE_PUBLISHABLE_KEY=YOUR_STRIPE_PUBLISHABLE_KEY
STRIPE_PRICE_ID=YOUR_STRIPE_PRICE_ID
